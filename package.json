{
  "name": "node-js-jwt-auth-thimble",
  "version": "1.0.0",
  "description": "Node.js backend for Thimble",
  "main": "server.js",
  "scripts": {
    "server": "node server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently --names \"server,client\" \"npm run server \" \"npm run client\"",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "concurrently --names \"server,watch\" \"npm run server \" \"npm run watch\"",
    "watch": "cd client && npm run start"
  },
  "keywords": [
    "node.js",
    "jwt",
    "authentication",
    "express",
    "mysql"
  ],
  "author": "alexander",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^6.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "path": "^0.12.7",
    "sequelize": "^5.21.3"
  }
}
